result-view{
	match: All (this)
	
	render{
		layout{
			section{
				content {
					cell-area {
						slot1 {
							image {
								url("[#{value(this.random_pic.url)}]")
								shape (Square)
							}
						}
						slot2 {
							content {
								primary ("[#{value(this.text)}]")
							}
						}
					}
					
				}
			}
		}
	}
}





















