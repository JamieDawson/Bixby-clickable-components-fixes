result-view{
  match: All (this)

  render{
    layout{
      section{
        content {


          paragraph{
            value("#{value(this.text)}")
          }
          image{
            url("#{value(this.random_pic.url)}")
            lightbox-enabled (false) //This makes the image unclickable
          }

          
        }
      }
    }
  }
}